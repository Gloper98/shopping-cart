{{# each products }}
<div class="row">
    {{# each this }}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail card card-padding card-margin">
      <img class="image-responsive" src="{{ this.imagePath }}" alt="picture">
      <div class="caption">
        <h3>Thumbnail label</h3>
        <p class="description">Lorem ipsum dolor sit amet consectetur adipisicing elit. Atque illum neque suscipit eaque totam. Laboriosam temporibus minima repellat molestiae aperiam quibusdam facilis earum ipsum debitis? Omnis ad non ullam cum!</p>
        <div class="clearfix">
            <div class="price float-left">$12</div>
            <a href="#" class="btn btn-success float-right" role="button">Button</a>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
{{/each}}