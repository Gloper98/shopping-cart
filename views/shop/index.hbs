{{# each products }}
<div class="row">
    {{# each this }}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail card card-padding card-margin">
      <img class="image-responsive" src="{{ this.imagePath }}" alt="picture">
      <div class="caption">
        <h3>{{ this.title }}</h3>
        <p class="description">{{ this.description }}</p>
        <div class="clearfix">
            <div class="price float-left">${{ this.price }}</div>
            <a href="#" class="btn btn-success float-right" role="button">Add to shopping cart</a>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
{{/each}}